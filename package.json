{
  "name": "darkbot",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "private": "true",
  "scripts": {
    "build": "swc src -d build",
    "watch": "swc src -d build -w",
    "dev": "run-s build start",
    "start": "node --enable-source-maps .",
    "clean": "node scripts/clean.mjs",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext ts --fix"
  },
  "imports": {
    "#root/*": "./build/*.js",
    "#lib/*": "./build/lib/*.js",
    "#constants/*": "./build/lib/constants/*.js",
    "#env/*": "./build/lib/env/*.js",
    "#utils/*": "./build/lib/utils/*.js"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@sapphire/eslint-config": "^4.3.7",
    "@sapphire/prettier-config": "^1.4.3",
    "@sapphire/ts-config": "^3.3.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.213",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "@sapphire/decorators": "^4.3.6",
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/framework": "next",
    "@sapphire/plugin-logger": "^2.2.3",
    "@sapphire/time-utilities": "^1.7.4",
    "@sapphire/utilities": "^3.6.2",
    "@skyra/env-utilities": "^1.0.1",
    "colorette": "^2.0.19",
    "discord-api-types": "^0.36.1",
    "discord.js": "^13.8.1",
    "fastify": "^4.2.1",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.0"
  },
  "engines": {
    "node": ">=16.x"
  },
  "prettier": "@sapphire/prettier-config",
  "eslintConfig": {
    "extends": "@sapphire",
    "ignorePatterns": [
      "node_modules/",
      "*build/",
      "*dist/",
      "*prod/"
    ]
  }
}
